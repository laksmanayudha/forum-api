{
  "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true,
      "jest": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
      "ecmaVersion": "latest"
  },
  "rules": {
      "linebreak-style": "off",
      "no-console": "off",
      "no-underscore-dangle": "off",
      "no-shadow": "off",
      "class-methods-use-this": "off",
      "no-unused-vars": "off"
  }
}
